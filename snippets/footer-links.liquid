<div class="mobile-collapse">
  <h3 class="text-left  title-under  mobile-collapse__title">{{ settings[title] }}</h3>
  <div class="v-links-list mobile-collapse__content">
    <ul>
      {% for link in linklists[settings[footerLinks]].links %}
      <li><a href="{{ link.url }}">{{ link.title }}</a></li>
      {% endfor %}
    </ul>
  </div>
</div>